{
  "name": "article_summary",
  "strict": true,
  "schema": {
    "type": "object",
    "properties": {
      "title": { "type": "string" },
      "summary": { "type": "string" },
      "key_points": { "type": "array", "items": { "type": "string" }, "maxItems": 6 },
      "entities": { "type": "object", "additionalProperties": { "type": "array", "items": { "type": "string" } } }
    },
    "required": ["summary", "key_points"],
    "additionalProperties": false
  }
}
